
@{
    ViewBag.Title = "UploadFile";
}

<h2>UploadFile</h2>

@using(Html.BeginForm("UploadFile","Upload", FormMethod.Post, new { enctype="multipart/form-data"}))
{
<div>
    @Html.TextBox("file", "", new { type = "file", required = "required" }) <br />
    @Html.TextBox("xslfile", "", new { type = "file", required = "required" }) <br />
    <input type="submit" value="Upload" /> <br />
    @ViewBag.message
</div>
}
